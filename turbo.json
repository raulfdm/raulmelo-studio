{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build", "typegen"],
      "outputs": [
        "dist/**",
        "build/**",
        ".svelte-kit/**",
        "functions/**",
        ".vercel"
      ]
    },
    "dev": {
      "outputs": [],
      "dependsOn": ["typegen"]
    },
    "typegen": {
      "inputs": ["src/**/*.tsx", "src/**/*.ts"],
      "outputs": ["src/**/*.typegen.ts", ".svelte-kit"]
    },
    "lint": {
      "outputs": []
    },
    "check": {
      "outputs": []
    },
    "test": {
      "inputs": ["src/**/*.tsx", "src/**/*.ts"]
    },
    "deploy": {
      "dependsOn": ["^build"],
      "outputs": []
    }
  }
}
