{
	"name": "fitness-buddy",
	"version": "1.0.0-development",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "run-p dev:*",
		"dev:svelte": "vite dev --host",
		"dev:typegen": "pnpm run typegen --watch",
		"build": "svelte-kit sync && vite build",
		"preview": "vite preview",
		"sync": "svelte-kit sync",
		"check": "pnpm run typegen && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"typegen": "xstate typegen \"src/**/*.ts?(x)\""
	},
	"dependencies": {
		"@raulmelo/sanity-core": "workspace:*",
		"@sanity/client": "6.4.12",
		"@sanity/vision": "3.16.7",
		"@tabler/icons-svelte": "2.34.0",
		"@xstate/cli": "0.5.2",
		"@xstate/svelte": "2.1.0",
		"cookie": "0.5.0",
		"daisyui": "2.52.0",
		"fuse.js": "6.6.2",
		"groq": "3.16.7",
		"lodash.clonedeep": "4.5.0",
		"react": "18.2.0",
		"sanity": "3.16.7",
		"sanity-plugin-media": "2.2.2",
		"uuid": "9.0.1",
		"xstate": "4.38.2",
		"zod": "3.22.2"
	},
	"devDependencies": {
		"@raulmelo/eslint-config": "workspace:*",
		"@sveltejs/adapter-auto": "2.1.0",
		"@sveltejs/kit": "1.25.0",
		"@tailwindcss/aspect-ratio": "0.4.2",
		"@types/cookie": "0.5.2",
		"@types/lodash.clonedeep": "4.5.7",
		"@types/uuid": "9.0.3",
		"@typescript-eslint/eslint-plugin": "6.7.4",
		"@typescript-eslint/parser": "6.7.4",
		"autoprefixer": "10.4.16",
		"eslint": "8.49.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-svelte": "2.33.1",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.30",
		"prettier": "3.0.3",
		"prettier-plugin-svelte": "3.0.3",
		"svelte": "4.2.0",
		"svelte-check": "3.5.1",
		"svelte-preprocess": "5.0.4",
		"tailwindcss": "3.3.3",
		"tslib": "2.6.2",
		"typescript": "5.2.2",
		"vite": "4.4.9",
		"vitest": "0.34.5"
	}
}
