{
	"name": "fitness-buddy",
	"version": "1.0.0-development",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "run-p dev:*",
		"dev:svelte": "vite dev --host",
		"dev:typegen": "bun run typegen --watch",
		"build": "svelte-kit sync && vite build",
		"preview": "vite preview",
		"sync": "svelte-kit sync",
		"check": "bun run typegen && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"test:unit": "bun test",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"typegen": "xstate typegen \"src/**/*.ts?(x)\""
	},
	"dependencies": {
		"@raulmelo/sanity-core": "workspace:*",
		"@sanity/client": "6.8.6",
		"@sanity/vision": "3.20.1",
		"@tabler/icons-svelte": "2.42.0",
		"@xstate/cli": "0.5.11",
		"@xstate/svelte": "2.1.0",
		"cookie": "0.6.0",
		"daisyui": "2.52.0",
		"fuse.js": "7.0.0",
		"groq": "3.20.1",
		"lodash.clonedeep": "4.5.0",
		"react": "18.2.0",
		"sanity": "3.20.1",
		"sanity-plugin-media": "2.2.4",
		"uuid": "9.0.1",
		"xstate": "4.38.3",
		"zod": "3.22.4"
	},
	"devDependencies": {
		"@raulmelo/eslint-config": "workspace:*",
		"@sveltejs/adapter-vercel": "3.1.0",
		"@sveltejs/kit": "1.30.3",
		"@tailwindcss/aspect-ratio": "0.4.2",
		"@types/cookie": "0.6.0",
		"@types/lodash.clonedeep": "4.5.9",
		"@types/uuid": "9.0.7",
		"@typescript-eslint/eslint-plugin": "6.12.0",
		"@typescript-eslint/parser": "6.12.0",
		"autoprefixer": "10.4.16",
		"eslint": "8.54.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-svelte": "2.35.1",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.31",
		"prettier": "3.0.3",
		"prettier-plugin-svelte": "3.0.3",
		"svelte": "4.2.8",
		"svelte-check": "3.5.2",
		"svelte-preprocess": "5.0.4",
		"tailwindcss": "3.3.5",
		"tslib": "2.6.2",
		"typescript": "5.2.2",
		"vite": "4.5.0"
	},
	"engines": {
		"node": "18",
		"pnpm": "8"
	},
	"volta": {
		"extends": "../../package.json"
	}
}
