{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "verbatimModuleSyntax": true,
    "moduleResolution": "bundler",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/ui": [
        "./src/ui"
      ],
      "@/infrastructure": [
        "./src/infrastructure"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": [
      "@astrojs/image/client"
    ],
    "skipLibCheck": true
  },
}