{
  "kind": "collectionType",
  "collectionName": "post_tags",
  "info": {
    "name": "Post Tag",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true,
      "regex": "^[\\w-_]*",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "required": true,
      "targetField": "name"
    },
    "blog_posts": {
      "collection": "post",
      "via": "post_tags"
    },
    "til_post": {
      "model": "til",
      "via": "tags"
    }
  }
}
